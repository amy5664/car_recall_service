<?xml version="1.0" encoding="UTF-8"?>
<!DOCTYPE mapper PUBLIC "-//mybatis.org//DTD Mapper 3.0//EN" "http://mybatis.org/dtd/mybatis-3-mapper.dtd">
<mapper namespace="com.boot.dao.AdminMapper">
    <select id="findByAdminId" resultType="com.boot.dto.UserDto">
        SELECT
            admin_id as username,
            admin_pw as password,
            'ROLE_ADMIN' as role
        FROM
            admin_account
        WHERE
            admin_id = #{adminId}
    </select>
</mapper>
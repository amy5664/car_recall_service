<?xml version="1.0" encoding="UTF-8" ?>
<!DOCTYPE mapper
        PUBLIC "-//mybatis.org//DTD Mapper 3.0//EN"
        "https://mybatis.org/dtd/mybatis-3-mapper.dtd">

<mapper namespace="com.boot.dao.AdminDAO">
    <resultMap id="adminResultMap" type="com.boot.dto.AdminDTO">
        <result property="admin_id" column="admin_id"/>
        <result property="admin_pw" column="admin_pw"/>
    </resultMap>

    <select id="login" resultMap="adminResultMap">
        SELECT
            admin_id,
            admin_pw
        FROM
            ADMIN_ACCOUNT
        WHERE
            admin_id = #{admin_id}
    </select>
</mapper>
